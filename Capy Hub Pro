local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

local Window = OrionLib:MakeWindow({Name = "Capy Scripts Hub V1", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})


--Tabs


local Tab1 = Window:MakeTab({
	Name = "Universal",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local Tab2 = Window:MakeTab({
	Name = "Nhân Vật",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local Tab3 = Window:MakeTab({
	Name = "Blox Fruits",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local Tab4 = Window:MakeTab({
	Name = "Bee Swarm",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local Tab5 = Window:MakeTab({
	Name = "A Dusty Trip",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})


--Sections


local Section = Tab1:AddSection({
	Name = "Universal Scripts"
})

local Section = Tab2:AddSection({
	Name = "Scripts Cho Nhân Vật"
})

local Section = Tab3:AddSection({
	Name = "Blox Fruits Scripts"
})

local Section = Tab4:AddSection({
	Name = "Bee Swarm Scripts"
})

local Section = Tab5:AddSection({
	Name = "A Dusty Trip Scripts"
})

--Buttons Tab1


Tab1:AddButton({
	Name = "Infinite Yeild",
	Callback = function()
      		loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))()
  	end    
})

