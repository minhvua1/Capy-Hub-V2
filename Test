local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

--window

local Window = Rayfield:CreateWindow({
   Name = "Capy Hub V2",
   LoadingTitle = "Loading Capy Hub V2",
   LoadingSubtitle = "by Capy",
   Theme = "DarkBlue", -- DarkBlue, Green, Light, Default - more coming soon!

   DisableRayfieldPrompts = false,

   ConfigurationSaving = {
      Enabled = false,
      FolderName = nil, -- Create a custom folder for your hub/game
      FileName = "Capy Hub"
   },

   Discord = {
      Enabled = false, -- Prompt the user to join your Discord server if their executor supports it
      Invite = "noinvitelink", -- The Discord invite code, do not include discord.gg/. E.g. discord.gg/ABCD would be ABCD
      RememberJoins = true -- Set this to false to make them join the discord every time they load it up
   },

   KeySystem = false, -- Set this to true to use our key system
   KeySettings = {
      Title = "Untitled",
      Subtitle = "Key System",
      Note = "No method of obtaining the key is provided", -- Use this to tell the user how to get a key
      FileName = "Key", -- It is recommended to use something unique as other scripts using Rayfield may overwrite your key file
      SaveKey = true, -- The user's key will be saved, but if you change the key, they will be unable to use your script
      GrabKeyFromSite = false, -- If this is true, set Key below to the RAW site you would like Rayfield to get the key from
      Key = {"Hello"} -- List of keys that will be accepted by the system, can be RAW file links (pastebin, github etc) or simple strings ("hello","key22")
   }
})

--tabs and sections

local UniTab = Window:CreateTab("Universal", 4483362458) -- Title, Image
local UniSection = UniTab:CreateSection("Slider")

local SupportTab = Window:CreateTab("H·ªó Tr·ª£", 4483362458) -- Title, Image
local SupportSection = SupportTab:CreateSection("TƒÉng Hi·ªáu NƒÉng")

local VuiTab = Window:CreateTab("Vui", 4483362458) -- Title, Image
local VuiSection = VuiTab:CreateSection("N·ªØa S·∫Ω C√≥ ü´∏üî¥üîµü´∑ü´¥üü£")

--Notifications

Rayfield:Notify({
   Title = "Phi√™n B·∫£n",
   Content = "V2.0",
   Duration = 15,
   Image = 4483362458,
   Actions = { -- Notification Buttons

      Ignore = { -- Duplicate this table (or remove it) to add and remove buttons to the notification.
         Name = "Hi·ªÉu R·ªìi",
         Callback = function()
            print("The user tapped Okay!")
         end
      },

},
})

--buttons unitab

--slider

local speedValue = 16  -- Gi√° tr·ªã m·∫∑c ƒë·ªãnh ban ƒë·∫ßu

local UniSlider = UniTab:CreateSlider({
   Name = "t·ªëc ƒë·ªô",
   Range = {0, 100},
   Increment = 1,
   Suffix = "Speed",
   CurrentValue = speedValue,
   Flag = "SpeedSlider",
   Callback = function(Value)
      speedValue = Value  
      if game.Players.LocalPlayer.Character then
         game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = speedValue  
      end
   end,
})

game.Players.LocalPlayer.CharacterAdded:Connect(function(character)
   character:WaitForChild("Humanoid").WalkSpeed = speedValue  
end)

if game.Players.LocalPlayer.Character then
   game.Players.LocalPlayer.Character:WaitForChild("Humanoid").WalkSpeed = speedValue
end



local jumpPowerValue = 50  -- Gi√° tr·ªã m·∫∑c ƒë·ªãnh cho nh·∫£y cao

local UniSlider = UniTab:CreateSlider({
   Name = "Nh·∫£y Cao",
   Range = {0, 200},
   Increment = 1,
   Suffix = "Jump Power",
   CurrentValue = jumpPowerValue,
   Flag = "JumpPowerSlider",
   Callback = function(Value)
      jumpPowerValue = Value 
      if game.Players.LocalPlayer.Character then
         game.Players.LocalPlayer.Character.Humanoid.JumpPower = jumpPowerValue  
      end
   end,
})

game.Players.LocalPlayer.CharacterAdded:Connect(function(character)
   character:WaitForChild("Humanoid").JumpPower = jumpPowerValue  
end)

if game.Players.LocalPlayer.Character then
   game.Players.LocalPlayer.Character:WaitForChild("Humanoid").JumpPower = jumpPowerValue
end



local gravityValue = 196

local UniSlider = UniTab:CreateSlider({
   Name = "Tr·ªçng L·ª±c",
   Range = {0, 500},
   Increment = 1,
   Suffix = "Gravity",
   CurrentValue = gravityValue,
   Flag = "GravitySlider",
   Callback = function(Value)
      gravityValue = Value
      workspace.Gravity = gravityValue
   end,
})

workspace.Gravity = gravityValue

game.Players.LocalPlayer.CharacterAdded:Connect(function()
   workspace.Gravity = gravityValue
end)

